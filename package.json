{
  "name": "emberfire",
  "version": "3.0.0",
  "description": "The officially supported adapter for using Firebase with Ember",
  "keywords": [
    "ember-addon",
    "firebase"
  ],
  "license": "MIT",
  "author": "Firebase (https://firebase.google.com/)",
  "homepage": "https://firebaseopensource.com/projects/firebase/emberfire/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firebase/emberfire.git"
  },
  "bugs": {
    "url": "https://github.com/firebase/emberfire/issues"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "serve": "ember serve",
    "package": "npm run build && npm pack .",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js:fix": "eslint . --fix",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "ts:precompile": "ember ts:precompile",
    "reference": "typedoc --options typedoc.js addon"
  },
  "dependencies": {
    "broccoli-file-creator": "^2.1.1",
    "broccoli-funnel": "^2.0.2",
    "broccoli-merge-trees": "^3.0.2",
    "ember-auto-import": "^1.5.2",
    "ember-cli-babel": "^7.26.6",
    "firebase": "^7.0.0"
  },
  "devDependencies": {
    "@ember/test-helpers": "^2.4.2",
    "@embroider/test-setup": "^0.43.5",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@types/ember": "^3.1.1",
    "@types/ember-data": "^3.1.8",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.1",
    "ember-ajax": "^4.0.2",
    "ember-auto-import": "^1.11.3",
    "ember-cli": "^3.28.0",
    "ember-cli-code-coverage": "^0.4.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-qunit": "^4.4.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript": "^1.5.0",
    "ember-data": "^3.13.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^6.7.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^6.2.2",
    "ember-resolver": "^8.0.2",
    "ember-simple-auth": "^1.9.2",
    "ember-source": "^3.28.0",
    "ember-template-lint": "^3.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "qunit": "^2.16.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^1.2.1",
    "typescript": "^3.6.3",
    "ws": "^6.2.1",
    "xmlhttprequest": "^1.8.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "fastbootDependencies": []
  },
  "typings": "index.d.ts"
}
